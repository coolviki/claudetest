{
  "name": "stock-portfolio-tracker",
  "version": "1.0.0",
  "description": "A web app to track realized and unrealized profits from Indian stock transactions",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd server/client && npm start",
    "build": "cd server/client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd client && npm install"
  },
  "keywords": [
    "stock",
    "portfolio",
    "tracker",
    "indian-stocks",
    "profit"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
